{
  "name": "freepaint",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.18"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "scripts": {
    "start": "bun run src/server/index.ts",
    "dev": "bun run --watch src/server/index.ts",
    "build": "bun build src/client/index.ts --outdir=public/dist --target=browser --format=iife --minify",
    "watch": "bun build --watch src/client/index.ts --outdir=public/dist --target=browser --format=iife --minify"
  }
}